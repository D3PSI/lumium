{
    "name": "lumium-space",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run lumium-renderer:install && next build",
        "start": "next start",
        "test": "jest -c test/jest.config.js",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:test": "npm run lumium-renderer:install && concurrently -k -s first 'npm start' 'npm run cypress:run'",
        "lumium-renderer:package": "cd lumium-renderer && wasm-pack build && cd pkg && export LUMIUM_RENDERER_TGZ=$(npm pack | tail -n 1) && cp $LUMIUM_RENDERER_TGZ ../../lumium-renderer.tgz",
        "lumium-renderer:install": "npm run lumium-renderer:package && npm update lumium-renderer",
        "preinstall": "npm run lumium-renderer:package"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.8.8",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@next/bundle-analyzer": "^12.1.6",
        "@reduxjs/toolkit": "^1.8.2",
        "axios": "^0.21.4",
        "concurrently": "^7.2.1",
        "framer-motion": "^3.10.6",
        "lumium-renderer": "file:lumium-renderer.tgz",
        "next": "^12.1.6",
        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^7.0.1",
        "nextjs-cors": "^2.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.39.1",
        "react-redux": "^7.2.8",
        "supertokens-auth-react": "^0.22.0",
        "supertokens-node": "^9.2.3",
        "swr": "^0.4.2"
    },
    "devDependencies": {
        "@testing-library/react": "11.2.7",
        "@testing-library/react-hooks": "5.1.3",
        "@types/node": "16.11.38",
        "@types/react": "17.0.45",
        "@types/react-dom": "17.0.17",
        "@types/react-redux": "7.1.24",
        "cypress": "6.9.1",
        "dotenv": "8.6.0",
        "eslint": "8.17.0",
        "eslint-config-next": "12.1.6",
        "identity-obj-proxy": "3.0.0",
        "isomorphic-unfetch": "3.1.0",
        "jest": "26.6.3",
        "nock": "13.2.6",
        "start-server-and-test": "1.14.0",
        "ts-jest": "26.5.6",
        "typescript": "4.7.3"
    }
}
